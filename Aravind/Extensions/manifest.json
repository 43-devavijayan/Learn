{
    "name": "My new Extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["link.js"]
    },

    "browser_action": {
        "browser_style": true,
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
         
    },

    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens hello.html"
        }
    }
}
